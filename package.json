{
  "name": "react-hexagons",
  "version": "1.0.0",
  "author": "nicolas1999gigena@gmail.com",
  "scripts": {
    "assets": "yarn install && yarn start",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.babel.js -p --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "clean": "npm run test:clean && npm run build:clean",
    "eslint:fix": "eslint -- . --ignore-path .gitignore --fix",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:js": "npm run lint:eslint -- . ",
    "start": "node server",
    "start:prod": "node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "test": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test:watch": "jest --watchAll",
    "generate": "plop --plopfile config/generators/index.js",
    "prettier:check": "prettier -c 'app/**/*.js'",
    "prettier:run": "prettier --write 'app/**/*.jsx' && prettier --write 'app/**/*.js'"
  },
